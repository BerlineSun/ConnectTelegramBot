<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Telegram.Bot" Version="18.0.0" />
    <PackageReference Include="Telegram.Bot.Extensions.Polling" Version="1.0.2" />
	  <Using Include="Telegram.Bot" />
	  <Using Include="ConnectTelegramBot.Obj" Static="true" />
	  <Using Include="Telegram.Bot.Types" />
	  <Using Include="ConnectTelegramBot.BotReceive"/>
	  <Using Include="Telegram.Bot.Types.Enums"/>
	  <Using Include="Telegram.Bot.Polling"/>
  </ItemGroup>
	
</Project>
